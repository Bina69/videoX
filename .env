# Port the app listens on
PORT=10000

# X_COOKIE: paste the cookie string you copied from browser here (DO NOT commit to Git)
# Format example (replace with your real values):
# X_COOKIE=auth_token=YOUR_AUTH_TOKEN; ct0=YOUR_CT0; personalization_id=YOUR_PID; guest_id=YOUR_GUEST_ID
X_COOKIE=

# Numeric user id (not username). Example: 44196397
X_USER_ID=

# Cache file name (relative to app folder)
CACHE_FILE=videos.json

# Cache TTL in seconds (600 = 10 minutes)
CACHE_EXPIRE=600
